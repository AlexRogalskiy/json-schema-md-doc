{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [
            "./lib/JSONSchemaMarkdown.js"
        ]
    },
    "sourceType": "module",
    "opts": {
        "template": "node_modules/docdash",
        "destination": "docs/documentaion/"
    },
    "docdash": {
        "sectionOrder": [
            "Tutorials",
            "Classes",
            "Modules",
            "Externals",
            "Events",
            "Namespaces",
            "Mixins",
            "Interfaces"
        ],
        "sort": false,
        "private": false,
        "search": true,
        "menu": {
            "GitHub": {
                "href": "https://github.com/brianwendt/json-schema-md-doc",
                "target": "_blank",
                "class": "menu-item",
                "id": "website_link"
            }
        }
    }
}